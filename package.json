{
  "name": "mailpit-api",
  "version": "1.0.7",
  "description": "A NodeJS client library, written in TypeScript, to interact with the Mailpit API.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mpspahr/mailpit-api.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "echo \"TODO: Add tests\" && exit 0",
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup_type",
    "pretty": "npx prettier . --write"
  },
  "keywords": [
    "mailpit-api",
    "mailpit",
    "api",
    "client",
    "library",
    "wrapper",
    "email",
    "typescript",
    "nodejs"
  ],
  "author": "Matthew Spahr",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/node": "^20.14.15",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.18.0"
  }
}
